<src:AbstractMainForm  x:Class="Minder.UI.SkinController.MainForms.WhiteSkin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:src="clr-namespace:Minder.UI.SkinController"
        Title="WhiteSkin" Height="80" Width="211" BorderThickness="0" WindowStyle="None" WindowStartupLocation="CenterScreen" AllowsTransparency="True" ShowInTaskbar="False" Left="Auto">
    <Grid Margin="0,0,2,2">
        <TextBox x:FieldModifier="private" Height="35" Margin="7,41,5,2" x:Name="m_textBox" Width="197" Background="{x:Null}" TextWrapping="Wrap" />
        <Label Content="Unavailable" x:FieldModifier="private" Height="28" HorizontalAlignment="Left" Margin="62,10,0,0" x:Name="m_dateLabel" VerticalAlignment="Top" Width="135" FontWeight="Bold" />
        <Label Content="New task" Height="28" HorizontalAlignment="Left" Margin="7,10,0,0" x:Name="label1" VerticalAlignment="Top" Width="81" />

    </Grid>
</src:AbstractMainForm>
